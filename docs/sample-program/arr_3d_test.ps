class Program:  
    private int[4, 4, 4] arr  
    private int a
    private int b
    private int len

    public static int Main():
        arr[1, 1, 2] = 5
        PrintArr(2)  
        return 0  


    private static int PrintArr(int c):
        a = 0
        len = 4
        while (a < len):
            b = 0            
            while (b < len):
                Console.WriteInt(arr[a,b, c])
                Console.WriteSpace()
                b = b + 1
            Console.WriteLine()
            a = a + 1
        return 0