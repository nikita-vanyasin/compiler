class Program:  
    private int[5, 5, 5] arr  
    private int a
    private int b
    private int c
    private int len
    private int max

    public static int Main():
        len = 5
        FillArr()

        c = 0
        while (c < len):
            PrintArrPage(c)
            c = c + 1
        Console.WriteString("Max number is: ")
        FindMax()
        Console.WriteInt(max)
        Console.WriteLine()
        return 0  

    private static int FillArr():
        a = 0
        while (a < len):
            b = 0            
            while (b < len):
                c = 0
                while (c < len):
                    arr[a, b, c] = Math.Rand()
                    c = c + 1
                b = b + 1
            a = a + 1
        return 0

    private static int FindMax():
        a = 0
        while (a < len):
            b = 0            
            while (b < len):
                c = 0
                while (c < len):
                    if (max < arr[a,b,c]):
                        max = arr[a,b,c]
                    c = c + 1
                b = b + 1
            a = a + 1
        return 0

    private static int PrintArrPage(int page):
        Console.WriteString("page ")
        Console.WriteInt(page)
        Console.WriteString(":")
        Console.WriteLine()

        a = 0
        while (a < len):
            b = 0            
            while (b < len):
                Console.WriteInt(arr[a,b, page])
                Console.WriteSpace()
                b = b + 1
            Console.WriteLine()
            a = a + 1
        Console.WriteLine()
        return 0