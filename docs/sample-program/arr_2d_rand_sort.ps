class Program:  
    private int[10, 10] a  
    private int i
    private int j
    private int len
    private bool swapped 
    private int iter
    private int temp

    public static int Main():
        len = 10
        FillArr()
        PrintArr()
        Console.WriteLine()
        Console.WriteLine()
        SortArr()
        PrintArr()
        return 0

    private static int FillArr():
        i = 0
        while (i < len):
            j = 0            
            while (j < len):
                a[i, j] = Math.Rand()
                j = j + 1
            i = i + 1
        return 0

    private static int PrintArr():
        i = 0
        while (i < len):
            j = 0            
            while (j < len):
                Console.WriteInt(a[i,j])
                Console.WriteSpace()
                j = j + 1
            Console.WriteLine()
            i = i + 1
        return 0

    private static int SortArr():
        i = 0
        while (i <= len):
            swapped = true
            len = 9
            iter = 0
            while(swapped):
                swapped = false
                while(iter <= (len - 1)):
                    if(a[i, iter] > a[i, iter + 1]):
                        temp = a[i, iter]
                        a[i, iter] = a[i, iter + 1]
                        a[i, iter + 1] = temp
                        swapped = true
                    iter = iter + 1
                iter = 0  
            i = i + 1           
        return 0
